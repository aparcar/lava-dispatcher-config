{% extends 'base.jinja2' %}

{% block vland %}
{#  #}
{% endblock vland %}

{% block body %}
device_type: dragonboard-410c
adb_serial_number: '{{ adb_serial_number|default('0000000000') }}'
fastboot_serial_number: '{{ fastboot_serial_number|default('0000000000') }}'
fastboot_options: {{ fastboot_options|default([]) }}

actions:
  deploy:
    methods:
      lxc:
      fastboot:
    connections:
      lxc:
      serial:
  boot:
    connections:
      lxc:
      serial:
    methods:
      lxc:
      fastboot:
        - no-flash-boot
        - auto-login
        - shell-session
        - export-env

{% endblock body %}
